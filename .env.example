
# API URL - Using proxy in development
VITE_API_URL=/api

# For production, use the full URL
# VITE_API_URL=https://your-backend.com/api

# Environment
VITE_APP_ENV=development

# Application name
VITE_APP_NAME=ChatEmbed

# Laravel backend URL (used for direct connections)
VITE_LARAVEL_URL=http://localhost:8000

# Mock API mode - set to false to use real Laravel backend
VITE_USE_MOCK_API=false

# Debug mode for API services
VITE_API_DEBUG=true
